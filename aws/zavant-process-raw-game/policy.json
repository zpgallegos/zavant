{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "s3:GetObject",
        "Resource": "arn:aws:s3:::zavant-games-raw/*"
      },
      {
        "Effect": "Allow",
        "Action": "s3:PutObject",
        "Resource": [
          "arn:aws:s3:::zavant-game-boxscore/*",
          "arn:aws:s3:::zavant-game-data/*",
          "arn:aws:s3:::zavant-game-players/*",
          "arn:aws:s3:::zavant-game-teams/*",
          "arn:aws:s3:::zavant-play-events/*",
          "arn:aws:s3:::zavant-play-info/*",
          "arn:aws:s3:::zavant-play-runners/*"
        ]
      }
    ]
  }